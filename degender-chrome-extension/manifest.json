{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "scripts/jquery-1.8.3.js", "scripts/fl.core.js", "scripts/jquery.fl.js", "scripts/jquery.fl.proofreader.js", "scripts/jquery.fl.div.js", "scripts/jquery.fl.iframe.js", "scripts/widget.js", "scripts/autoproof.js", "scripts/fl-chrome.js" ],
