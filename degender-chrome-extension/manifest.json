      "128": "images/128x128.png",
      "16": "images/16x16.png",
      "48": "images/48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "DeGender",
   "options_page": "options/options.html",
   "page_action": {
      "default_icon": "images/16x16.png",
      "default_title": "DeGender - schöne deutsche Sprache auf Knopfdruck",
